{
  "name": "pepeverde/libs",
  "description": "Librerie per lo sviluppo di applicazioni",
  "type": "library",
  "version": "0.19.0-dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Manuel Dalla Lana",
      "email": "manuel@dallalana.it"
    },
    {
      "name": "Matteo Agostinetto",
      "email": "tecnico@pepeverde.agency"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-fileinfo": "*",
    "ext-imagick": "*",
    "ext-mbstring": "*",
    "lib-pcre": "*",
    "aura/session": "~4.0",
    "behat/transliterator": "^1.5.0",
    "endelwar/zigra": "0.10.*",
    "imagecow/imagecow": "^2.4",
    "pelago/emogrifier": "^7.0",
    "diablomedia/doctrine1": "^4.0.4",
    "sentry/sdk": "3.1.*",
    "soundasleep/html2text": "~2.1",
    "swiftmailer/swiftmailer": "v6.3.0",
    "twig/twig": "^v3.17.1",
    "vlucas/phpdotenv": "^5.6.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.65",
    "phpstan/phpstan": "^1.12",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Pepeverde\\": "src/Pepeverde/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pepeverde\\Test\\": "tests/Pepeverde"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
