{
  "name": "pepeverde/libs",
  "description": "Librerie per lo sviluppo di applicazioni",
  "type": "library",
  "version": "0.15.3",
  "license": "proprietary",
  "authors": [
    {
      "name": "Manuel Dalla Lana",
      "email": "manuel@pepeverde.agency"
    },
    {
      "name": "Matteo Agostinetto",
      "email": "tecnico@pepeverde.agency"
    }
  ],
  "require": {
    "php": "^5.6|^7.0|^7.1|^7.2",
    "ext-mbstring": "*",
    "lib-pcre": "*",
    "aura/session": "~2.1",
    "behat/transliterator": "~1.2",
    "endelwar/zigra": "~0.8.4",
    "pelago/emogrifier": "~2.0",
    "pepeverde/doctrine1": "^1.3.14",
    "sentry/sentry": "~1.9",
    "soundasleep/html2text": "~0.5",
    "swiftmailer/swiftmailer": "~5.4",
    "twig/twig": "^1.35|^2.5",
    "vlucas/phpdotenv": "~2.5",
    "imagecow/imagecow": "^2.1",
    "ext-imagick": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12|^2.13",
    "phpunit/phpunit": "^5.7.27|^6.5|^7.5"
  },
  "autoload": {
    "psr-4": {
      "Pepeverde\\": "src/Pepeverde/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pepeverde\\Test\\": "tests/Pepeverde"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
