{
  "name": "pepeverde/libs",
  "description": "Librerie per lo sviluppo di applicazioni",
  "type": "library",
  "version": "0.16.1",
  "license": "proprietary",
  "authors": [
    {
      "name": "Manuel Dalla Lana",
      "email": "manuel@dallalana.it"
    },
    {
      "name": "Matteo Agostinetto",
      "email": "tecnico@pepeverde.agency"
    }
  ],
  "require": {
    "php": "^7.2|^7.3|^7.4",
    "ext-fileinfo": "*",
    "ext-imagick": "*",
    "ext-mbstring": "*",
    "lib-pcre": "*",
    "aura/session": "~2.1",
    "behat/transliterator": "~1.3",
    "endelwar/zigra": "~0.8.6",
    "imagecow/imagecow": "^2.4",
    "pelago/emogrifier": "~2.2",
    "pepeverde/doctrine1": "^1.3.16",
    "sentry/sdk": "2.0.4",
    "soundasleep/html2text": "~0.5",
    "swiftmailer/swiftmailer": "~5.4",
    "twig/twig": "^2.14",
    "vlucas/phpdotenv": "~2.6"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Pepeverde\\": "src/Pepeverde/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pepeverde\\Test\\": "tests/Pepeverde"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
